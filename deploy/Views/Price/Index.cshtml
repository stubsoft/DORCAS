@model IEnumerable<DorcasClub.Price>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.from)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.to)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.purchase_Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sale_Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.insertionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codeDevis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.product_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type_product)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Session.etat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.from)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.to)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.purchase_Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sale_Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.insertionDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.codeDevis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.product_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.type_product)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Session.etat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idPrice }) |
            @Html.ActionLink("Details", "Details", new { id=item.idPrice }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idPrice })
        </td>
    </tr>
}

</table>
